<%- include("header") -%>
<body>
<div class="box" id="heading">
      <h1> <%= listtitle %> </h1>
</div>

<div class="box">
  <% for(var i=0; i<listitems.length; i++){ %>
    <div class="item">
      <input type="checkbox">
        <p><%= listitems[i] %></p>
    </div>
    <% } %>
    <form action="/" method="post">
      <input type="text" name="input" placeholder="enter the next objective" autocomplete="off">
      <button type="submit" name="button" value=<%= listtitle %>>+</button>
    </form>
</div>
</body>
<%- include("footer") -%>
</html>
